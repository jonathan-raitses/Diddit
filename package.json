    "url": "https://github.com/jonathan-raitses/Diddit/issues"
  },
  "homepage": "https://github.com/jonathan-raitses/Diddit#readme",
  "dependencies": {
    "assert": "^1.4.1",
    "axios": "^0.18.0",
    "babel": "^6.23.0",
    "express": "^4.16.4",
    "mongodb": "^3.1.13",
    "morgan": "^1.9.1",
    "newrelic": "^5.2.1",
    "nodemon": "^1.18.9",
    "path": "^0.12.7",
    "react": "^16.7.0",
    "react-columns": "^1.2.0",
    "react-dom": "^16.7.0",
    "react-redux": "^6.0.0",
